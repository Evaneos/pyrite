{
    "name" : "evaneos/pyrite",
    "description" : "PHP Framework around Stack php principle",
    "type" : "library",
    "authors" : [{
        "name" : "Olivier Madre",
        "email" : "olivier@evaneos.com"
    }
    ],
    "keywords" : [
        "frameork",
        "php",
        "stack"
    ],
    "license" : [
        "MIT"
    ],
    "repositories" : [
        {
            "type": "vcs",
            "url": "git@github.com:Evaneos/dic-it.git"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Pyrite\\" : "src"
        }
    },
    "require" : {
        "evaneos/dic-it": "~2.0",
        "stack/builder": "~1.0",
        "symfony/http-foundation": "^2.8",
        "symfony/http-kernel": "^2.8",
        "symfony/routing": "^2.8",
        "symfony/yaml": "^2.8",
        "monolog/monolog": "~1.11",
        "twig/twig": "~1.0",
        "symfony/event-dispatcher": "^2.8",
        "stack/run": "^1.0",
        "symfony/dependency-injection": "^2.8"
    },
    "require-dev": {
        "symfony/debug": "^2.8",
		"symfony/var-dumper": "^2.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
