{
    "name" : "evaneos/pyrite",
    "description" : "PHP Framework around Stack php principle",
    "type" : "library",
    "authors" : [{
            "name" : "Olivier Madre",
            "email" : "olivier@evaneos.com"
        }
    ],

    "keywords" : [
        "frameork",
        "php",
        "stack"
    ],
    "license" : [
        "MIT"
    ],
    "repositories" : [
        {
            "type": "vcs",
            "url": "git@github.com:Evaneos/dic-it.git"
        }
    ],

    "autoload" : {
        "psr-4" : {
            "Pyrite\\" : "src"
        }
    },
    "require" : {
        "christophehurpeau/php-var-dump": "~1.0",
        "evaneos/dic-it": "~1.0.1",
        "stack/builder": "~1.0.0",
        "symfony/http-foundation": "~2.4.0",
        "symfony/http-kernel": "~2.4.0",
        "symfony/routing": "~2.4.0",
        "symfony/yaml": "2.5.x-dev",
        "symfony/debug": "~2.4.0",
        "monolog/monolog": "~1.11",
        "twig/twig": "1.*"
    },
    "require-dev": {
        "stack/session": "dev-master"
    },
    "minimum-stability" : "stable"
}
