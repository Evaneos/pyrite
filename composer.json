{
    "name" : "evaneos/pyrite",
    "description" : "PHP Framework around Stack php principle",
    "type" : "library",
    "authors" : [{
            "name" : "Joel Wurtz",
            "email" : "jwurtz@jolicode.com"
        }
    ],
    "keywords" : [
        "frameork",
        "php",
        "stack"
    ],
    "license" : [
        "MIT"
    ],
    "autoload" : {
        "psr-4" : {
            "Pyrite\\" : "src"
        }
    },
    "require" : {
        "oliviermadre/dic-it": "dev-master",
        "stack/builder": "~1.0.0",
        "symfony/http-foundation": "~2.4.0",
        "symfony/http-kernel": "~2.4.0",
        "symfony/routing": "~2.4.0",
        "symfony/yaml": "2.5.x-dev",
        "symfony/debug": "~2.4.0",
        "monolog/monolog": "1.7.0",
        "twig/twig": "1.*"
    },
    "require-dev": {
        "stack/session": "dev-master"
    },
    "minimum-stability" : "stable"
}
